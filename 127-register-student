public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c stu = new apxio__Student__c();
    stu.Name = name;
    stu.apxio__Email__c = email;
    stu.apxio__Phone__c = phone;
    
    insert stu;
      stu = [select Id, Name, apxio__Phone__c, apxio__Email__c,apxio__Registration_Number__c from apxio__Student__c where Id =: stu.Id ];
     return stu.apxio__Registration_Number__c ;

}