public Id classFromCourse(String courseName) {
    // code here
    apxio__Course__c course = [Select id, Name from apxio__Course__c Where Name =:courseName];
    if(course.Id != null){
        apxio__Class__c class = new apxio__Class__c();
        class.apxio__Course__c = course.Id;
        class.Name = course.Name;
        insert class;
        return class.Id;
    }else{
        return null;
    }
}